package com.github.spocot.mythra;

public enum Direction {
	
	NORTH(0,-1),SOUTH(0,1),EAST(1,0),WEST(-1,0),
	;
	
	private int xVar;
	private int yVar;
	
	private Direction(int xVar, int yVar){
		this.xVar = xVar;
		this.yVar = yVar;
	}
	
	public int xVar(){return xVar;}
	public int yVar(){return yVar;}
}
